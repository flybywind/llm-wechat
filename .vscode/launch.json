  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      "console": "integratedTerminal"
    },
    {
      "name": "test_app",
      "type": "debugpy",
      "request": "launch",
      "program": "test_robot_app.py",
      "justMyCode": false,
      "console": "integratedTerminal"
    }
  ]
}
